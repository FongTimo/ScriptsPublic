local UserInputService = game:GetService("UserInputService")

local function checkEPressed()
    return UserInputService:IsKeyDown(Enum.KeyCode.E)
end

while checkEPressed() do
local args = {
    [1] = game:GetService("Players").LocalPlayer.Character.SteelDagger,
    [2] = 4,
    [3] = 0.09999999999999998,
    [4] = game:GetService("Players").LocalPlayer.Character.SteelDagger.Model.AtkPart,
    [5] = "Nil"
}

game:GetService("ReplicatedStorage"):WaitForChild("Events"):WaitForChild("WeaponEvent"):FireServer(unpack(args))

    wait()  -- Ждем некоторое время перед повторением
end
